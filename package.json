{
  "name": "nextjs-nestjs-migration-example",
  "version": "0.1.0",
  "private": true,
  "bin": "server.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "pkg": "pkg --compress GZip --public --public-packages \"*\" ."
  },
  "pkg": {
    "assets": [
      ".next/**/*"
    ],
    "scripts": [
      ".next/**/*.js"
    ],
    "outputPath": "pkg",
    "targets": [
      "node16-win-x64"
    ]
  },
  "dependencies": {
    "@nestjs/common": "^8.4.4",
    "@nestjs/core": "^8.4.4",
    "@nestjs/platform-express": "^8.4.4",
    "next": "^12.1.5",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "@types/node": "^17.0.25",
    "@types/react": "^18.0.6",
    "pkg": "^5.6.0",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
